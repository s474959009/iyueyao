
<%- include header %>

<div class="container index-section">
    <script src="http://cdn.staticfile.org/jquery/2.0.3/jquery.js"></script>
    <script src="http://cdn.staticfile.org/twitter-bootstrap/3.0.2/js/bootstrap.min.js"></script>
    <div class="row">
        <div class="col-md-8 col-md-offset-2 blog-content">
            <h3 class="blog-title"><%= post.title %></h3>
            <p class="info">日期：<%= post.time.minute %></p>
            <%- post.post %>
        </div>
    </div>

    <%- include comment %>
    <%- include footer %>