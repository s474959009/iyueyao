
<%- include header %>
<style>

</style>
<div class="container index-section">
    <script src="http://cdn.staticfile.org/jquery/2.0.3/jquery.js"></script>
    <script src="http://cdn.staticfile.org/twitter-bootstrap/3.0.2/js/bootstrap.min.js"></script>
    <div class="row">
        <div class="col-md-8 col-md-offset-2">
            <% var lastYear = 0 %>
            <% posts.forEach(function (post, index) { %>
            <% if (lastYear != post.time.year) { %>
            <li><h3><%= post.time.year %></h3></li>
            <% lastYear = post.time.year } %>
            <li><time><%= post.time.day %></time></li>
            <li><a href="/<%= post.type %>/<%= post.uuid %>"><%= post.title %></a></li>
            <% }) %>
        </div>
    </div>
    <%- include footer %>