
<%- include header %>

<div class="container index-section">
    <h1><%= title %></h1>
    <p>欢迎来到这里哦 <%= title %></p>
    <% posts.forEach(function (post, index) { %>
    <p><h2><a href="/<%= post.type %>/<%= post.time.day %>/<%= post.title %>"><%= post.title %></a></h2></p>
    <p class="info">
        作者：<a href="#"><%= post.name %></a> |
        日期：<%= post.time.minute %>
    </p>
    <p><%- post.post %></p>
    <% }) %>
    <script src="http://cdn.staticfile.org/jquery/2.0.3/jquery.js"></script>
    <script src="http://cdn.staticfile.org/twitter-bootstrap/3.0.2/js/bootstrap.min.js"></script>

<%- include footer %>